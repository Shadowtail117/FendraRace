<?xml version="1.0" encoding="utf-8" ?>
<Defs>
<HediffDef ParentName="AddedBodyPartBase">
    <defName>FendraBionicTail</defName>
    <label>bionic fendra tail</label>
    <description>An installed bionic tail.</description>
    <spawnThingOnRemoved>FendraBionicTail</spawnThingOnRemoved>
    <addedPartProps>
        <solid>true</solid>
        <partEfficiency>1.1</partEfficiency>
        <betterThanNatural>true</betterThanNatural>
    </addedPartProps>
    <stages>
        <li>
            <capMods>
                <li>
                    <capacity>Moving</capacity>
                    <offset>0.1</offset>
                </li>
            </capMods>
        </li>
    </stages>
</HediffDef>
<HediffDef ParentName="AddedBodyPartBase">
    <defName>FendraClothTail</defName>
    <label>cloth fendra tail</label>
    <description>An installed cloth tail.</description>
    <addedPartProps>
        <solid>true</solid>
        <partEfficiency>0.6</partEfficiency>
    </addedPartProps>
</HediffDef>

<HediffDef>
    <defName>FendraClotting</defName>
    <label>fendra clotting</label>
    <description>A special compound in this fendra's blood speeds up the clotting process. Wait...how are you seeing this?</description>
    <isBad>false</isBad>
    <stages>
        <li>
            <totalBleedFactor>0.75</totalBleedFactor>
            <becomeVisible>false</becomeVisible>
        </li>
    </stages>
</HediffDef>
</Defs>